CODE :

#include <bits/stdc++.h>
using namespace std;

int maxRec(int a[], int n) {
    if(n==1) return a[0];
    return max(a[n-1], maxRec(a, n-1));
}

int minRec(int a[], int n) {
    if(n==1) return a[0];
    return min(a[n-1], minRec(a, n-1));
}

int main() {
    int n;
    cin >> n;
    int a[n];
    for(int i=0;i<n;i++) cin >> a[i];
    cout << "Max: " << maxRec(a,n) << "\n";
    cout << "Min: " << minRec(a,n);
}
