CODE :

#include <iostream>
using namespace std;

int main() {
    int matrix[3][3] = {
        {1, 2, 8}, {3, 6, 10}, {7, 9, 12}
    };
    int target = 9, n = 3, m = 3;
    int i = 0, j = m - 1;
    while (i < n && j >= 0) {
        if (matrix[i][j] == target) {
            cout << "Activated";
            return 0;
        } else if (matrix[i][j] > target) j--;
        else i++;
    }
    cout << "Failed";
}
