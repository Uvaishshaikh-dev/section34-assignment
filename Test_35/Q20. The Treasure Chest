CODE :

#include <iostream>
using namespace std;

int main() {
    int arr[] = {1, 2, 2, 2, 3}, key = 2;
    int n = 5, low = 0, high = n - 1, ans = -1;
    while (low <= high) {
        int mid = (low + high) / 2;
        if (arr[mid] == key) {
            ans = mid;
            low = mid + 1;
        } else if (arr[mid] < key) low = mid + 1;
        else high = mid - 1;
    }
    cout << ans;
}
